.acclaim-section {
    .relative {
        .vert {
            padding-top: 2rem;
            font-weight: 600;
            @media screen and (min-width:767px){
                font-size: 76px;
            }
        }
    }
    .tech {
        margin-bottom: 5rem;
        @media screen and (max-width:767px){
            margin-bottom: 0;
            padding-top:0;
        }
        .tech-item  {
            position: relative;
            @media screen and (min-width:1500px){
                padding: 4rem 3rem 0 6rem;
            }
            @media screen and (max-width:1440px){
                padding: 4rem 3rem 0 4rem;
            }
            @media screen and (max-width:767px){
                padding: 2rem 0rem 0 0rem;
                border-top: 0;
            }
            &:before {
                content: '';
                position: absolute;
                top: 60px;
                right: 71px;
                background-image: url({{ 'acclime-icon.png' | asset_url }});
                background-position: center;
                background-size: contain;
                background-repeat: no-repeat;
                width: 45px;
                height: 135px;
                @media screen and (max-width:1440px){
                    width: 50px;
                    top: 60px;
                }
                @media screen and (max-width:767px){
                    right: 0;
                    top: 13px;
                }
            }
            .spec{
                h5 {
                    border-bottom: 1px solid #eeeeee;
                    padding-bottom: 12px;
                    font-size: 20px;
                    @media screen and (max-width:1440px){
                        font-size: 18px;
                    }
                }
                p{
                    padding-right: 80px;
                    font-size: 14px;
                    @media screen and (max-width:767px){
                        padding-right: 70px;
                    }
                    @media screen and (max-width:1440px){
                        font-size: 12px;
                    }
                }
            }
        }
    }
}
