.gallery-slider {
    z-index: 9999;
    .product-thumbs.pdp-gallery-nav {
        .slick-prev {
            display: none;
        }
        .slick-next {
            font-size: 0;
            height: 45px;
            border-radius: 50%;
            border: 1px solid #ac8b33;
            width: 45px;
            display: block;
            margin-left:auto;
            margin-right:auto;
            position: relative;
            z-index: 99;
            background: none;
            &:before {
                content: "";
                position: absolute;
                z-index: 2;
                color: #b18e3a;
                border: solid #b18e3a;
                border-width: 0 2px 2px 0;
                display: block;
                padding: 3px;
                width: 15px;
                height: 15px;
                margin: 19px 0 0 20px;
                top: -8px;
                right: 14px;
                transform: rotate(45deg);
            }
        }
    }
}

.gallery-slider.soft-prod-gallery{
    width: 50%;
    padding-right: 0;
    transform: translateX(-50%);
    left: 50%;
    top: 15%;
    .product-thumbs.swiper-wrapper{
        border: 2px double #ac8b33;
    }
        .product-thumbs.swiper-slide{
            display: inline-table;
            border: 1px solid #ac8b33;
            margin-bottom: 30px !important;
            &:before {
                border: 1px solid #ac8b33;
                content: "";
                position: absolute;
                width: calc(100% - 10px);
                height: calc(100% - 10px);
                z-index: -1;
                left: 5px;
                top: 5px;
            }
            .product-thumb{
                display: inline-block;
                margin-bottom: 0px;
                padding: 8px;
                border: none;
                width: 48%;
            }
        }
        .gallery-thumb-name{
            font-size: 15px;
            color: #c2c2c2;
            font-weight: bold;
            display: block;
            text-align: center;
            padding: 15px 10px;
        }
    }
    .pdp-gallery-nav {
        .swiper-slide.product-thumb {
            margin-bottom: 10px;
            border: 1px solid #ac8b33;
            padding: 4px;
        }
    }
    .pdp-gallery {
        .slick-prev {
            right: 100px;
            top: 20px;
            font-size: 0;
            height: 45px;
            bottom: auto;
            border-radius: 50%;
            border: 1px solid #ac8b33;
            width: 45px;
            transform: rotate(0);
            &::after {
                top: -7px !important;
                right: 8px !important;
                width: 20px !important;
                height: 20px !important;
                margin: 19px 0 0 20px!important;
                transform: rotate(135deg);
            }
        }
        .slick-next {
            position: absolute;
            right: 40px;
            top: 20px;
            font-size: 0;
            height: 45px;
            bottom: auto;
            border-radius: 50%;
            border: 1px solid #ac8b33;
            width: 45px;
            background: 0 0;
            transform: rotate(0);
            &::after{
                content: "";
                position: absolute;
                z-index: 2;
                color: #b18e3a;
                border: solid #b18e3a;
                border-width: 0 2px 2px 0;
                display: block;
                padding: 3px;
                width: 20px;
                height: 20px;
                margin: 19px 0 0 20px;
                top: -7px;
                right: 14px;
                transform: rotate(-45deg);
            }
        }
    }
