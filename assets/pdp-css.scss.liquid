.pdp-logos-slider {
    padding: 40px 0;
    max-width: 850px;
    margin: 40px auto; 
    .pdp-logo-slide {
        padding: 0 20px;
        img {
            max-width: 255px;
            margin: 0 auto;
        }
    }
    .slick-track {
        display: flex;
        align-items: center;
    }
    .slick-list {
        padding:0 50px;
    }
    .slick-arrow {
        background-color:transparent;
        cursor: pointer;
        background-image: url( {{ 'slider-btn.jpg' | asset_url }} );
        background-repeat:no-repeat;
        background-position:center;
        transform: none;
        top:0;
        position: absolute;
        left:-30px;
        width:85px;
        border: 0;
        font-size: 0;
        height: 100%;
        &:after {
            display: none;
        }
        @media only screen and (max-width: 991px) {
            left: -15px;
            top: -25px;
            background-color: #FFF;
        }
        @media only screen and (max-width: 767px) {
            top: 0px;
        }
        &.slick-next {
            left: auto;
            right: -30px;
            background-image: url(slider-btn2.jpg);
            @media only screen and (max-width: 991px) {
                right: -15px;
                top: -25px;
                background-color: #FFF;
            }
            @media only screen and (max-width: 767px) {
                top: 0px;
            }
        }
    }
}
