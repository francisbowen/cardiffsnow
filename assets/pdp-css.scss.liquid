.sticky-bar-pdp {    
    display: none;
    position: fixed;
    top: 100px;
    left: 0;
    width: 100%;
    background: #fafafa;
    border-bottom: 1px solid #ac8b33;
    z-index: 20;
    &.fixed {
        display:block;
    }
    @media only screen and (max-width: 767px){
        display: none !important;
    }
    .product-title {
        max-height: 35px;
        overflow: hidden;
        margin-bottom: 8px;
    }
    .stick-bar-title {
        padding-top: 20px; 
        padding-left: 50px;
    }
    .product-add {
        height: 100%;
        .select-overlay {
            font-size: 25px;
            @media screen and (max-width:1500px){
                font-size: 20px;
            }
        }
    }
    .product-variants {
        padding-top: 20px;
        justify-content: center;
        width: 100%;
        .product-variant {
            margin-right: 2rem;
            @media screen and (max-width:991px){
                margin-right: 1rem;
                margin-left: 1rem;
            }
            .product-variant-title {
                margin-left:-10px;
                font-size: 1rem;
                @media screen and (max-width:1500px){
                    font-size: 15px;
                }
            }
            .product-options {
                select {
                    width: 140px;
                    height: 40px;
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    padding: 0 12px;
                    outline: none;
                    border-color: #ac8b33;
                    color: #ac8b33;
                    &[disabled="disabled"] {
                        opacity: 0.25;
                    }
                    @media screen and (max-width:1500px){
                        height: 35px;
                    }
                } 
            }
        }
    }
}
