.instagram-story {    
    margin: 10px 0 100px 0;
    padding-left: 200px;
    @media only screen and (max-width: 767px){
        padding-left: 0;
        margin-bottom: 70px;
    }
    .position-relative {
        position: relative !important;
    }
    .heading {
        font-family: meno-banner,serif;
        font-size: 4rem;
        color: #ac8b33;
        font-weight: bold;
        position: absolute;
