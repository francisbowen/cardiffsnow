.template-product-soft-goods,
.template-product-hard-goods {
    #wrap {
        padding-top: 5rem; 
    }
    .product-thumbs {
        display: table-cell;
        .swiper-slide.product-thumb {
            display: inline-block;
            margin-bottom: 20px;
            padding: 8px;
            border: 2px double #ac8b33;
            width: 150px;
            .improduct-single__thumbnail-imageg:hover {
                border: 0;
            }
        }
    }
    #MainContent {
        .pdp-explore {
            padding: 5rem 0;
            &:after {
                display: block;
                content: "";
                position: absolute;
                top: 36%;
                left: 0;
                width: 64%;
                height: 44%;
                z-index: -1;
                background: #eee;
                @media only screen and (max-width: 767px){
                    display: none;
                }
            }
            h3 {
                font-size: 3rem;
                @media only screen and (max-width: 767px){
                    font-size: 1rem;
                    margin-bottom: 0;
                }
            }
            a {
                font-size: 1rem;
                @media only screen and (max-width: 767px){
                    font-size: .5rem;
                }
                .arrow  {
                    @media only screen and (max-width: 767px){
                        padding: 0 0 0 .5rem;
                        max-width: 3rem;
                    }
                }
            }
        }
    }    
    .bindings-products {
        display: flex;
        @media only screen and (max-width: 767px) {
            flex-wrap: wrap;
            margin:0 !important;
        }
        .product-binding {
            margin-left: 60px;
            position: relative;
            width: 35%;
            @media only screen and (max-width: 767px){
                margin:0 10px;
                width: 44%;
            }
            .product-binding-img {
                background-position: center;
                background-size: contain;
                padding:60% 0;
                background-repeat: no-repeat;
                position: relative;
                a {
                    position: absolute;
                    left:0;
                    top:0;
                    width: 100%;
                    bottom: 0;
                }
            }
            .product-binding-detail {
                display: flex;
                justify-content: space-between;
                border-bottom: 1px solid #ac8b33;
                padding-bottom: 16px;
                margin-bottom: 15px;
                @media only screen and (max-width: 767px) {
                    padding-bottom: 10px;
                    margin-bottom: 10px;
                }
                .title,.price {
                    font-weight: 700;
                    font-size: 16px;
                    @media only screen and (max-width: 767px) {
                        font-size: 12px;
                    }
                }
            }
            p {
                margin: 0;
                @media only screen and (max-width: 767px) {
                    font-size: 11px;
                }
            }
            span {
                position: absolute;
                top: 0;
                right: 0;
                background-color: #ac8b33;
                color: #fff;
                padding: 5px 12px;
                transform: rotate(-90deg);
                font-size: 15px;
                @media only screen and (max-width: 767px) {
                    top: 3px;
                    right: -12px;
                    padding: 4px 10px;
                    font-size: 10px;
                }
            }
        }
    }
}

