                padding: 0;
                border-bottom: 1px solid transparent;
                svg {
                    display: none;
                }
            }
            ul.child {
                display: none;
                position: absolute;
                top: 100%;
                z-index: 999999;
                left: -60px;
                background: #fff;
                margin: 0;
                width: 260px;
                height: auto;
                padding: 25px 0px;
                padding-left: 15px;
                .child {
                    padding: 0 0.8rem;
                    >a {
                        color: #ac8b33 !important;
                        font-weight: 700;
                        font-size: 20px;
                        text-transform: capitalize;
                        display: inline-block;
                        padding: 10px 0;
                        border: 0 !important;
                        &:hover {
                            color: #000 !important;
                        }
                    }

                }
            }
            ul.grandchild {
                padding: 0;
                margin: -6px 0 0 0;
                li {
                    padding: 0;
                    a {
                        color:#000 !important;
                        border-bottom: 0;
                        font-size: 14px;
                        font-weight: bold;
                        @media screen and (max-width:767px){
                            color:#fff !important;
                        }
                        &:hover {
                            color: #ac8b33 !important;   
                        }
                    }
                }
            }
            @media screen and (min-width:767px){
            .for-mobile {
                display: none;
            }
                &:hover {
                    ul {
                        display: block;
                    }
                    a {
                        color: #ac8b33;
                        border-bottom: 1px solid #ac8b33;
                        // line-height: 1.5;
                        // padding: 0 0.2rem;
                    }
                }
            }
            @media screen and (max-width:767px){
                .for-desktop {
                    display: none;
                }
                a {
                    font-size: 1.5rem;
                }
                &.active {
                    ul {
                        display: block;
                        text-align: center;
                        position: relative;
                        background: 0 0;
                        left: 0;
